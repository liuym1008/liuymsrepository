    "# 第六题\n",
    "L = [1, 2, 3, 4, 5]\n",
    "# 创建一个新的空列表\n",
    "new_L1 = []\n",
    "# for循环\n",
    "for item in L[::-1]:\n",
    "    new_L1.append(item)\n",
    "print(new_L1)\n",
    "# 创建一个新的空列表\n",
    "new_L2 = []\n",
    "# while循环\n",
    "i = len(L) - 1\n",
    "while i >= 0:\n",
    "    new_L2.append(L[i])\n",
    "    i -= 1\n",
    "print(new_L2)"
   ]
  }
