    "# 第二周作业第二题\n",
    "score = int(input(\"please input your score: \"))\n",
    "if (score < 60):\n",
    "    print(f\"{score}为不合格\")\n",
    "elif (60 <= score <= 74):\n",
    "    print(f\"{score}为合格\")\n",
    "elif (75 <= score <= 89):\n",
    "    print(f\"{score}为良好\")\n",
    "else:\n",
    "    print(f\"{score}为优秀\")"
   ]
